  "optionalDependencies": {
    "rustite.win64.node": "1.0.0",
    "rustite-linux-x64-gnu": "1.0.0",
    "rustite-darwin-arm64": "1.0.0",
    "rustite-android-arm64": "1.0.0",
    "rustite-linux-arm64-gnu": "1.0.0",
    "rustite-linux-arm64-musl": "1.0.0",
    "rustite-win32-arm64-msvc": "1.0.0",
    "rustite-linux-arm-gnueabihf": "1.0.0",
    "rustite-linux-x64-musl": "1.0.0",
    "rustite-android-arm-eabi": "1.0.0"
